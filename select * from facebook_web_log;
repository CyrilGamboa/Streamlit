select * from facebook_web_log;
SELECT user_id,action,timestamp,
    EXTRACT(DAY FROM timestamp) as "Jour", 
    EXTRACT(MONTH FROM timestamp) as "Mois", 
    EXTRACT(YEAR FROM timestamp) as "Ann√©e", 
    EXTRACT(HOUR FROM timestamp) as "Heure",
    EXTRACT(MINUTE FROM timestamp) as "Minutes",
    EXTRACT(SECOND FROM timestamp) as "Seconde",   
   
FROM facebook_web_log;